<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.cx.erp.entities.TListFormationType_Entity" table="t_list_formation_type" schema="person">
        <id name="id">
            <column name="id" sql-type="int(2)"/>
        </id>
        <property name="formationType">
            <column name="formation_type" sql-type="char(10)" length="10" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>